{
  "name": "safe-map",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/safe-map.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "npm run prettier-production && npm run test && tsdx build",
    "test": "tsdx test",
    "test-watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prettier": "prettier --write \"{**/*,*}.{md,ts}\"",
    "prettier-production": "prettier --list-different \"{**/*,*}.{md,ts}\"",
    "prettier-staged": "pretty-quick --staged --pattern \"{**/*,*}.{md,ts}\""
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.137",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsdx": "^0.8.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "kanskje": "^2.0.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.21"
  }
}
